<!DOCTYPE html>
<html lang="en">
    <head>
        <!-- head/meta goes here -->
        {% include "./_parts/head.njk" %}
        <!-- // -->
    </head>
    <body>
        <header>
            <!-- Header goes here -->
            {% include "./_parts/header.njk" %}
            <!-- // -->
        </header>
        <content id="home">
            <section class="intro_wrapper">
                <section id="intro_block" class="home-section">
                    <div id="intro_heading">
                        {% renderFile "./_src/_includes/_parts/home/heading.md" %}
                    </div>
                    <div id="intro_android">
                        <img id="android_head" src="/img/aec.webp" width="260px" alt="android docs">
                    </div>
                    <div id="intro_subheading">
                        {% renderFile "./_src/_includes/_parts/home/subheading.md" %}
                    </div>
                    <div id="intro_cta">
                        <h3 class="cta">
                            <a class="button" href="/support">Get in touch</a>
                        </h3>
                    </div>
                    <div id="intro_text">
                            {{ content | safe }}
                    </div>
                </section>
            </section>

            <section id="partner_logos">
                <!--div class="home-subheading">
                    {% renderFile "./_src/_includes/_parts/home/partner-logos.md" %}
                </div-->
                {% include "./_parts/trustedlogos.njk" %}
            </section>

            <section id="what_i_do" class="home-section">
                <div class="home-heading">
                    {% renderFile "./_src/_includes/_parts/home/what-i-do-heading.md" %}
                </div>
                <div class="home-subheading">
                    {% renderFile "./_src/_includes/_parts/home/what-i-do-subheading.md" %}
                </div>
                <div id="highlight_container">
                    <div class="highlight-section" id="what_i_do_docs">
                        <div class="section-title">
                            <h2>Documentation & resources</h2>
                        </div>
                        <div class="highlight-section-content-text">
                            {% renderFile "./_src/_includes/_parts/home/what-i-do-docs.md" %}
                        </div>
                    </div>
                    <div class="highlight-section highlight-offside" id="">
                        <div class="highlight-offside-heading">
                            Recently updated
                        </div>
                        {% for post in collections["android-docs"] | reverse %}
                            {% if loop.index0 < 5 %}
                            <div class="post-block">
                                <div class="post-body">
                                    <div class="post-meta post-meta-condensed">
                                        <div class="meta-single">
                                            <i class="fas fa-edit"></i> {{ post.date | dateFull }}
                                        </div>
                                        <div class="meta-single">
                                            {% for tag in post.data.tags %}
                                            {% if not tag.includes("android-docs") %}
                                            #<a class="meta-tag" href="/tags/{{ tag | slugify }}/">{{ tag }}</a>
                                            {% endif %}
                                            {% endfor %}
                                        </div>
                                    </div>
                                    <div class="post-title">
                                        <a href="{{ post.url }}">{{ post.data.title }}</a>
                                    </div>
                                </div>
                            </div>
                            {% endif %}
                        {% endfor %}
                    </div>
                    <div class="highlight-section" id="what_i_do_community">
                        <div class="section-title">
                            <h2>Community</h2>
                        </div>
                        <div class="highlight-section-content-text">
                            {% renderFile "./_src/_includes/_parts/home/what-i-do-community.md" %}
                        </div>
                    </div>
                    <div class="highlight-section" id="what_i_do_consultancy">
                        <div class="section-title">
                            <h2>Consultancy</h2>
                        </div>
                        <div class="highlight-section-content-text">
                            {% renderFile "./_src/_includes/_parts/home/what-i-do-consultancy.md" %}
                        </div>
                    </div>
                </div>
            </section>

            <section id="posts" class="home-section">
                <h2 class="section-title">
                    Latest articles 
                </h2>
                <div id="post_container">
                    <div class="post-column">
                    {% for post in collections.blog | reverse %}
                    {% if loop.index0 < 3 %}
                        <div class="post-block">
                            <div class="post-body">
                                <div class="post-meta post-meta-condensed">
                                    <div class="meta-single">
                                        {{ post.date | dateFull }}
                                    </div>
                                    <div class="meta-single">
                                        {% for tag in post.data.tags %}
                                        {% if not tag.includes("blog") %}
                                        #<a class="meta-tag" href="/tags/{{ tag | slugify }}/">{{ tag }}</a>
                                        {% endif %}
                                        {% endfor %}
                                    </div>
                                </div>
                                <h3 class="post-title">
                                    <a href="{{ post.url | url }}">{{ post.data.title }}</a>
                                </h3>
                                {% if post.data.excerpt|length %}
                                <div class="post-summary">
                                    {{ post.data.excerpt }}
                                </div>
                                {% endif %}
                            </div>
                        </div>
                    {%- endif -%}
                    {%- endfor -%}
                    </div>
                    <div class="post-column">
                    {% for post in collections.blog | reverse | offset(3) %}
                    {% if loop.index0 < 3 %}
                        <div class="post-block">
                            <div class="post-body">
                                <div class="post-meta post-meta-condensed">
                                    <div class="meta-single">
                                        {{ post.date | dateFull }}
                                    </div>
                                    <div class="meta-single">
                                        {% for tag in post.data.tags %}
                                        {% if not tag.includes("blog") %}
                                        #<a class="meta-tag" href="/tags/{{ tag | slugify }}/">{{ tag }}</a>
                                        {% endif %}
                                        {% endfor %}
                                    </div>
                                </div>
                                <h3 class="post-title">
                                    <a href="{{ post.url | url }}">{{ post.data.title }}</a>
                                </h3>
                                {% if post.data.excerpt|length %}
                                <div class="post-summary">
                                    {{ post.data.excerpt }}
                                </div>
                                {% endif %}
                            </div>
                        </div>
                    {% endif %}    
                    {% endfor %}
                    </div>
                </div>
                <h3 class="post-title post-archive">
                    <a class="button" href="/blog">‚úçÔ∏è Archive ¬ª</a>
                </h3>
            </section>

            <section id="hero_banner" class="home-section">
                <h2 class="section-title">
                    Videos 
                </h2>
                <div id="af_group">
                    <div id="banner_left">
                        {% renderFile "./_src/_includes/_parts/home/vidtext.md" %}
                    </div>
                    <div id="banner_right">
                        <div id="af_text">
                            {% renderFile "./_src/_includes/_parts/home/video.md" %}
                        </div>
                    </div>
                </div>
                <div id="af_group_split">
                    <div id="video_left">
                        {% renderFile "./_src/_includes/_parts/home/video2.md" %}
                    </div>
                    <div id="banner_right">
                        <div id="video_right">
                            {% renderFile "./_src/_includes/_parts/home/video3.md" %}
                        </div>
                    </div>
                </div>
            </section>

            <section id="trusted_logos" class="home-section">
                <div id="trusted_container">
                    <div id="trusted_textgroup">
                        <h2 class="section-title">
                            Docs and resources
                        </h2>
                    </div>
                    <div id="doc_container">
                        <div class="post-column">
                            {% for post in collections["android-docs"] | reverse %}
                            {% if loop.index0 < 5 %}
                            <div class="post-block">
                                <div class="post-body">
                                    <div class="post-meta post-meta-condensed">
                                        <div class="meta-single">
                                            {{ post.date | dateFull }}
                                        </div>
                                        <div class="meta-single">
                                            {% for tag in post.data.tags %}
                                            {% if not tag.includes("android-docs") %}
                                            #<a class="meta-tag" href="/tags/{{ tag | slugify }}/">{{ tag }}</a>
                                            {% endif %}
                                            {% endfor %}
                                        </div>
                                    </div>
                                    <h3 class="post-title">
                                        <a href="{{ post.url }}">{{ post.data.title }}</a>
                                    </h3>
                                    {% if post.data.excerpt|length %}
                                    <div class="post-summary">
                                        {{ post.data.excerpt }}
                                    </div>
                                    {% endif %}
                                </div>
                            </div>
                            {% endif %}
                            {% endfor %}
                        </div>
                        <div class="post-column">
                        {% for post in collections["android-docs"] | reverse | offset(5) %}
                        {% if loop.index0 < 5 %}
                        <div class="post-block">
                            <div class="post-body">
                                <div class="post-meta post-meta-condensed">
                                    <div class="meta-single">
                                        {{ post.date | dateFull }}
                                    </div>
                                    <div class="meta-single">
                                        {% for tag in post.data.tags %}
                                        {% if not tag.includes("android-docs") %}
                                        #<a class="meta-tag" href="/tags/{{ tag | slugify }}/">{{ tag }}</a>
                                        {% endif %}
                                        {% endfor %}
                                    </div>
                                </div>
                                <h3 class="post-title">
                                    <a href="{{ post.url | url }}">{{ post.data.title }}</a>
                                </h3>
                                {% if post.data.excerpt|length %}
                                <div class="post-summary">
                                    {{ post.data.excerpt }}
                                </div>
                                {% endif %}
                            </div>
                        </div>
                    {% endif %}    
                    {% endfor %}
                    </div>
                    </div>
                </div>
                <h3 class="post-title post-archive">
                    <a class="button" href="/android">üìö Docs ¬ª</a>
                </h3>
            </section>

        </content>
        <footer>
            {% include "./_parts/footer.njk" %}
        </footer>
    </body>
</html>
